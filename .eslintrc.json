{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": 0,
        "comma-dangle":"off",
        "func-names" :"off",
        "global-require":"off",
        "no-underscore-dangle":"off",
        "no-restricted-globals":"off"
    }
}
